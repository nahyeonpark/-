import java.io.*;
import java.util.*;

public class Main {
    static Scanner scan = new Scanner(System.in);
    static StringBuilder sb = new StringBuilder();
	
    static long[] memo;
    static int n;
     
    public static void main(String[] args) {
    	n = scan.nextInt();
    	
        memo = new long[n+1];
        
        memo[0] = 0;
        memo[1] = 1;
        
		for(int i = 2; i <= n; i++) {
    		memo[i] = memo[i-1] + memo[i-2];
		}
		
    	System.out.println(memo[n]);	
    }
}
